#!/bin/sh
#PBS -N RUN-$jobID-CCVIL
#PBS -q batch
cd /home/chenwx/qsub
echo Running on hosts `hostname`

if [ ! -d CCVIL ]; then  
	mkdir CCVIL  
fi  

tar xpfz pkg.tgz -C ./CCVIL
rm pkg.tgz

cd CCVIL
echo Directory is $PWD
make

sed "s/^functionToRun [0-9]*$/functionToRun $jobID/" configure.ini > temp
cat temp > configure.ini
echo Job ID: $jobID
cat configure.ini | grep '^functionToRun [0-9]*$'

echo Start Time is `date`
./main.out
echo Finish Time is `date`
